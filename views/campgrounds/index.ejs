<% layout ('layout/boilerplate') %>

    <h1>List with all campgrounds</h1>
    <div>
        <a href="/campgrounds/new">Add Campground</a>
    </div>
    <ul>
      <% for  (let campground of campgrounds) { %>
        <li><a href="/campgrounds/<%= campground._id %>"><%= campground.title %></a></li>
        <% }%>
    </ul>
